{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/order.schema.json",
  "title": "Shipment",
  "description": "Shipment for ShipIt",
  "type": "object",
  "properties": {
    "productId": {
      "type": "number",
      "minimum": 1000
    },
    "name": {
      "type": "string"
    },
    "addr": {
      "type": "string"
    },
    "zip": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "productId",
    "name",
    "addr",
    "zip"
  ]
}
